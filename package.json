{
  "name": "beat_box",
  "version": "1.0.0",
  "description": "Fun drums looper/sequencer with WebAudio API",
  "main": "server.js",
  "scripts": {
    "clear": "rm -rf ./public/bundle.js",
    "start": "node server.js",
    "dev:client": "nodemon --exec \"webpack-dev-server --mode development\"",
    "build": "npm run clear && webpack --mode production",
    "deploy": "gh-pages -d public",
    "heroku-prebuild": "npm install --only=dev",
    "lint": "eslint . --ext .js, .jsx",
    "lint:fix": "eslint --fix . --ext .js, .jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kukiron/beat_box.git"
  },
  "engines": {
    "node": "9.5.0",
    "npm": "5.6.0"
  },
  "author": "Kafil Uddin Kiron",
  "license": "ISC",
  "homepage": "https://kukiron.github.io/beat_box/",
  "greenkeeper": {
    "ignore": [
      "package-names",
      "you-want-me-to-ignore"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.10",
    "eslint-config-latest": "^1.0.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "gh-pages": "^1.1.0",
    "nodemon": "^1.19.1",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "express": "^4.16.3",
    "raven-js": "^3.23.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-mdl": "^1.11.0",
    "tone": "^0.11.11"
  }
}
