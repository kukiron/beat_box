{
  "name": "beat_box",
  "version": "1.0.0",
  "description": "Fun drums looper/sequencer with WebAudio API",
  "main": "server.js",
  "scripts": {
    "clear": "rm -rf ./build",
    "clear:dist": "rm -rf ./dist/bundle.js",
    "dev": "nodemon --watch *.js --exec \"webpack-dev-server\"",
    "start": "node server.js",
    "build": "npm run clear:dist && webpack -p",
    "build:watch": "webpack -p -w",
    "deploy": "gh-pages -d dist",
    "heroku-prebuild": "npm install --only=dev",
    "precommit": "npm run lint",
    "test": "jest --verbose",
    "lint": "eslint . --ext .js, .jsx",
    "lint:fix": "eslint --fix . --ext .js, .jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kukiron/beat_box.git"
  },
  "author": "Kafil Uddin Kiron",
  "license": "ISC",
  "homepage": "https://beatbox-k.herokuapp.com",
  "greenkeeper": {
    "ignore": ["package-names", "you-want-me-to-ignore"]
  },
  "dependencies": {
    "express": "^4.16.2",
    "raven-js": "^3.22.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-mdl": "^1.11.0",
    "tone": "^0.11.11"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.9",
    "eslint-config-latest": "^1.0.6",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "nodemon": "^1.14.11",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  }
}
